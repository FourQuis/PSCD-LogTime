{
  "name": "cattr/backend-application",
  "description": "Backend Application for Cattr",
  "license": "SEE LICENSE IN LICENSE",
  "type": "project",
  "homepage": "https://cattr.app",
  "support": {
    "email": "hi@cattr.app",
    "docs": "https://docs.cattr.app/backend#/",
    "forum": "https://community.cattr.app/public/",
    "issues": "https://git.amazingcat.net/AmazingTime/core/cattr-backend/-/issues",
    "chat": "https://cattr.app",
    "source": "https://git.amazingcat.net/AmazingTime/core/cattr-backend"
  },
  "require": {
    "php": "~8.2",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "czproject/git-php": "^4.0",
    "erusev/parsedown": "^1.7",
    "flugger/laravel-responder": "dev-master",
    "matanyadaev/laravel-eloquent-spatial": "^3.1",
    "intervention/image": "^2.7",
    "joshbrw/laravel-module-installer": "^2.0",
    "laravel/framework": "^10.0",
    "laravel/octane": "^1.5",
    "laravel/sanctum": "^3.2",
    "laravel/telescope": "^4.14",
    "laravel/tinker": "^2.8",
    "maatwebsite/excel": "^3.1",
    "mpdf/mpdf": "^8.0",
    "nwidart/laravel-modules": "^10",
    "sentry/sentry-laravel": "^3.2",
    "webpatser/laravel-uuid": "^4.0",
    "wikimedia/composer-merge-plugin": "^2.0",
    "guzzlehttp/guzzle": "^7.2",
    "league/fractal": "0.19.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "itsgoingd/clockwork": "^5.1",
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-latest",
    "sempro/phpunit-pretty-print": "^1.0",
    "squizlabs/php_codesniffer": "^3",
    "nunomaduro/collision": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeds/"
    }
  },
  "scripts": {
    "dumphelpers": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "joshbrw/laravel-module-installer": true,
      "wikimedia/composer-merge-plugin": true,
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    },
    "gitlab-domains": [
      "git.amazingcat.net"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://git.amazingcat.net/api/v4/group/cattr/-/packages/composer/"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "merge-plugin": {
      "include": [
        "./Modules/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  }
}
